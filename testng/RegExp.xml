<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="SuiteRegExp">
  <test thread-count="5" name="TestRegExp">
  <groups>
  <run>
  <include name="firefox.*"></include>
  <include name="firefox.Sanity"></include>
  <include name="chrome.Smoke"></include>
